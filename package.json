{
  "name": "studio-app",
  "version": "0.0.1",
  "private": true,
  "main": "index.tsx",
  "scripts": {
    "compile": "tsc --noEmit -p . --pretty",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "patch": "patch-package",
    "adb": "adb reverse tcp:9090 tcp:9090 && adb reverse tcp:3000 tcp:3000 && adb reverse tcp:9001 tcp:9001 && adb reverse tcp:8081 tcp:8081",
    "postinstall": "patch-package",
    "build:ios:sim": "eas build --profile development --platform ios --local",
    "build:ios:dev": "eas build --profile development:device --platform ios --local",
    "build:ios:prod": "eas build --profile production --platform ios --local",
    "build:android:sim": "eas build --profile development --platform android --local",
    "build:android:dev": "eas build --profile development:device --platform android --local",
    "build:android:prod": "eas build --profile production --platform android --local",
    "start": "expo start --no-dev --minify",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "bundle:web": "npx expo export --platform web",
    "serve:web": "npx serve dist",
    "prebuild:clean": "npx expo prebuild --clean",
    "emu:android:start": "$ANDROID_HOME/emulator/emulator -avd automationTest -no-audio > /dev/null 2>&1 &",
    "emu:android:install:ReleaseApp": "adb uninstall com.studioapp ; adb install -r ./android/app/build/outputs/apk/release/app-release.apk && adb shell am start -n com.studioapp/.MainActivity",
    "android:release": "cd android && ./gradlew assembleRelease -x lint --build-cache",
    "android:fixed_for_pablo": "cd android ;export RUN=0 ; while [ $RUN -ne 99 ]; do let RUN=$RUN+1; echo \"ejecucion  $RUN\" ; sleep 10 ;  npm run android:release && echo \"funciono a la $RUN \" &&export RUN=99 ; done",
    "ios:release": "cd ios && xcodebuild -workspace StudioApp.xcworkspace -scheme StudioApp -sdk iphonesimulator -configuration release -resultBundlePath TestResults.xcresult -derivedDataPath ./out",
    "emu:ios:start": "open -a Simulator",
    "emu:ios:install:ReleaseApp": "xcrun simctl uninstall booted com.studioapp || true && xcrun simctl install booted ios/out/Build/Products/Release-iphonesimulator/StudioApp.app && xcrun simctl launch booted com.studioapp",
    "start:appium": "npx appium",

    "test": "npm run test:jest && npm run test:maestro && npm run test:appium",
    "test:jest": "jest",
    "test:jest:watch": "jest --watch",
    "test:maestro": "maestro test test/maestro/FavoritePodcast.yaml",
    "test:appium" : "wdio run ./test/appium/config/wdio.conf.ts",
    "test:local:appium" : "echo lol"
  },
  "dependencies": {
    "@expo-google-fonts/space-grotesk": "^0.2.2",
    "@expo/metro-runtime": "~4.0.0",
    "@react-navigation/bottom-tabs": "^6.3.2",
    "@react-navigation/native": "^6.0.2",
    "@react-navigation/native-stack": "^6.0.2",
    "@shopify/flash-list": "1.7.1",
    "apisauce": "3.0.1",
    "date-fns": "^4.1.0",
    "expo": "~52.0.7",
    "expo-application": "~6.0.1",
    "expo-build-properties": "~0.13.1",
    "expo-dev-client": "~5.0.1",
    "expo-font": "~13.0.1",
    "expo-linking": "~7.0.2",
    "expo-localization": "~16.0.0",
    "expo-splash-screen": "~0.29.10",
    "expo-status-bar": "~2.0.0",
    "expo-system-ui": "~4.0.3",
    "i18next": "^23.14.0",
    "intl-pluralrules": "^2.0.1",
    "mobx": "6.10.2",
    "mobx-react-lite": "4.0.5",
    "mobx-state-tree": "5.3.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-i18next": "^15.0.1",
    "react-native": "0.76.6",
    "react-native-drawer-layout": "^4.0.1",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-keyboard-controller": "^1.12.7",
    "react-native-mmkv": "^2.12.2",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-web": "~0.19.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@testing-library/react-native": "^12.5.2",
    "@types/jest": "^29.2.1",
    "@types/react": "~18.3.12",
    "@wdio/allure-reporter": "^9.5.0",
    "@wdio/appium-service": "^9.5.7",
    "@wdio/cli": "^9.5.7",
    "@wdio/cucumber-framework": "^9.5.0",
    "@wdio/local-runner": "^9.5.7",
    "@wdio/spec-reporter": "^9.5.0",
    "@wdio/visual-service": "^6.3.0",
    "appium": "^2.15.0",
    "appium-uiautomator2-driver": "^3.9.9",
    "appium-xcuitest-driver": "^8.1.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.57.0",
    "eslint-config-expo": "~8.0.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react-native": "^4.1.0",
    "eslint-plugin-reactotron": "^0.1.2",
    "jest": "^29.2.1",
    "jest-expo": "~52.0.1",
    "patch-package": "^8.0.0",
    "postinstall-prepare": "1.0.1",
    "prettier": "^3.3.3",
    "react-test-renderer": "18.2.0",
    "reactotron-core-client": "^2.9.4",
    "reactotron-mst": "^3.1.7",
    "reactotron-react-js": "^3.3.11",
    "reactotron-react-native": "^5.0.5",
    "reactotron-react-native-mmkv": "^0.2.6",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "~5.3.3",
    "webdriverio": "^9.5.7"
  },
  "engines": {
    "node": "^18.18.0 || >=20.0.0"
  }
}
